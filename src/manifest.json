{
  "manifest_version": 2,
  "name": "Picture in Picture",
  "description": "This extension will open a new tab for Picture in Picture mode",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "camera.png"
  },
  "icons": {
    "64": "camera.png"
  },
  "permissions": ["tabs"]
}
